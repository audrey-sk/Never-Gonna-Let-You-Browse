{
    "name": "Never Gonna Let You Browse",
    "description": "An extension that will gradually drive a person's brain to rot via maddness",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "js": ["scripts/redirect_link.js"],
          "matches": ["https://*/*"]
        },
        {
          "js": ["scripts/change_image.js"],
          "matches": ["https://*/*"]
        }
    ],
    "background": {
        "service_worker": "scripts/redirect_tab.js"
    },
    "permissions": ["tabs", "storage"],
    "options_page": "frontend/dashboard.html"
  }